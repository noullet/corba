import java.util.HashMap;
import java.util.Map;


public class Roman {

	public static String convert(int arabic) {
		String roman = "";
		List associations
		associations.put(5, "V");
		associations.put(4, "IV");
		associations.put(1, "I");
		
		if (arabic >= 5) {
			roman += "V";
			arabic -= 5;
		}
		if (arabic >= 4) {
			roman += "IV";
			arabic -= 4;
		}	
		while(arabic > 0) {
			if (arabic >= 1) {
				roman += "I";
				arabic --;
			}
		}
		return roman;
	}
	
	private class ArabicToRomanEntry {
		private int arabic;
		private String roman;
		public ArabicToRomanEntry(int arabic, String roman) {
this.roman = roman;
this.arabic = arabic;}
	}

}
