import java.util.ArrayList;
import java.util.List;


public class Roman {
	
	static private List<ArabicToRomanEntry> associations = new ArrayList<ArabicToRomanEntry>();

	static {
		associations.add(new ArabicToRomanEntry(50, "L"));
		associations.add(new ArabicToRomanEntry(40, "XL"));
		associations.add(new ArabicToRomanEntry(10, "X"));
		associations.add(new ArabicToRomanEntry(9, "IX"));
		associations.add(new ArabicToRomanEntry(5, "V"));
		associations.add(new ArabicToRomanEntry(4, "IV"));
		associations.add(new ArabicToRomanEntry(1, "I"));		
	}

	public static String convert(int arabic) {
		String roman = "";
		for(ArabicToRomanEntry entry : associations) {
			while(arabic > 0) {
				if (arabic >= entry.getArabic()) {
					roman += entry.getRoman();
					arabic -= entry.getArabic();
				}
			}
		}
		return roman;
	}
	
	
}

class ArabicToRomanEntry {
	
	private int arabic;
	private String roman;
	
	public ArabicToRomanEntry(int arabic, String roman) {
		this.roman = roman;
		this.arabic = arabic;
	}
	
	public int getArabic() {
		return arabic;
	}
	public String getRoman() {
		return roman;
	}
}
