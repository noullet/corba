import java.util.ArrayList;
import java.util.List;


public class Roman {

	public static String convert(int arabic) {
		String roman = "";
		List<ArabicToRomanEntry> associations = new ArrayList<ArabicToRomanEntry>();
		associations.add(new ArabicToRomanEntry(5, "V"));
		associations.add(new ArabicToRomanEntry(4, "IV"));
		associations.add(new ArabicToRomanEntry(1, "I"));
		
		while(arabic > 0) {
			for(ArabicToRomanEntry entry : associations) {
				if (arabic >= entry.getArabic()) {
					roman += entry.getRoman();
					arabic -= entry.getArabic();
				}
			}
		}
		return roman;
	}
	
	
}

class ArabicToRomanEntry {
	
	private int arabic;
	private String roman;
	
	public ArabicToRomanEntry(int arabic, String roman) {
		this.roman = roman;
		this.arabic = arabic;
	}
	
	public int getArabic() {
		return arabic;
	}
	public String getRoman() {
		return roman;
	}
}
